<style>
    .itemMaster{
        width: 40%;
        display: block;
        margin: 5% auto;
    }
</style>
</head>
<body onload="navibar()" class="antialiased bg-gray-100">
    <nav class="navbar bg-body-tertiary " data-bs-theme="dark">
        <div class="container-fluid">
          <a class="navbar-brand">Deshboard</a>
          <form class="d-flex" role="search">
         <a href="" class="text-light text-decoration-none">Logout</a>
          </form>
        </div>
      </nav>
    <div class="flex relative" x-data="{navOpen: false}">
        <!-- NAV -->
        <div id="deshboardData"></div>
         <form action="" method="post" class="itemMaster">
            <div class="mb-4">
                <label class="block text-gray-700 text-sm font-bold mb-2" for="username">
                  Item Name
                </label>
                <input required class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" id="username" name="username" type="text" placeholder="Name">
              </div>
              <div class="differant">
              <input type='file' name="img" id="uploadBannerImage" onchange="readURL(this);" />
              
                
                <select name="itemType" class="form-select mt-3" aria-label="Default select example">
                    <option selected>Select Item Type</option>
                    <option value="forrent">For Rent</option>
                    <option value="forsale">For Sale</option>
                    
                  </select>
              
            </div>
              <div class="mb-4">
                <label class="block text-gray-700 text-sm font-bold mb-1 mt-2" for="Price">
                  Price
                </label>
                <input required name="price" class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" id="us" type="text" placeholder="Price">
              </div>
              <div>
              <textarea required name="desc" id="" cols="53" rows="10"></textarea></div>
              <button type="submit" class="btn btn-primary mt-3">Submit</button>
            </form>
        </main>
    </div>
</body>
</html>